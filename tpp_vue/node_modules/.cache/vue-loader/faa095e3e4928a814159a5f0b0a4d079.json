\" :rfImg=\"require('../../assets/ic_search.png')\"  :rsImg=\"rsImg\"\r\n        :search=\"search\" :add3=\"add3\"\r\n        ></title-bar>\r\n        <!-- container -->\r\n        <mt-tab-container v-model=\"active\">\r\n            <mt-tab-container-item id=\"message\">\r\n                <message-list></message-list>\r\n            </mt-tab-container-item>\r\n        </mt-tab-container>\r\n        <!-- tabbar -->\r\n        <mt-tabbar v-model=\"active\" fixed>\r\n            <!-- <mt-tab-item v-for=\"(elem,i) of tabbaricon.data\" :key=\"i\" :id=\"elem.id\">\r\n                <tab-icon :normalImage=\"elem.normalImage\" :selectImage=\"elem.selectImage\" :focused=\"active==elem.id \"></tab-icon>\r\n                {{elem.msg}}\r\n            </mt-tab-item> -->\r\n            <mt-tab-item id=\"message\" @click.native=\"changeState(0)\">\r\n                <tab-icon :normalImage=\"normalImage[0]\" :selectImage=\"selectImage[0]\" :focused=\"currentIndex[0].isSelect\"></tab-icon>\r\n                消息\r\n            </mt-tab-item>\r\n            <mt-tab-item id=\"contact \" @click.native=\"changeState(1)\">\r\n                <tab-icon :normalImage=\"normalImage[1]\" :selectImage=\"selectImage[1]\" :focused=\"currentIndex[1].isSelect\"></tab-icon>通讯录</mt-tab-item>\r\n            <mt-tab-item id=\"find\"   @click.native=\"changeState(2)\">\r\n                <tab-icon :normalImage=\"normalImage[2]\" :selectImage=\"selectImage[2]\" :focused=\"currentIndex[2].isSelect\"></tab-icon>发现</mt-tab-item>\r\n            <mt-tab-item id=\"me\"   @click.native=\"changeState(3)\">\r\n                <tab-icon :normalImage=\"normalImage[3]\" :selectImage=\"selectImage[3]\" :focused=\"currentIndex[3].isSelect \"></tab-icon>我</mt-tab-item>\r\n        </mt-tabbar>\r\n    </div>\r\n</template>\r\n<script>\r\nimport titleBar from \"./common/Titlebar\";\r\nimport messageList from \"./common/MessageList\";\r\nimport tabIcon from \"./common/TabBarIcon\";\r\nimport tabbaricon from \"./json/tabbar.json\";\r\nexport default {\r\n    data(){\r\n        return {\r\n            currentIndex:[\r\n                // 分别保存4个按钮的状态\r\n                {isSelect:true},{isSelect:false},{isSelect:false},{isSelect:false}\r\n            ],\r\n            tabbaricon,\r\n            focused:\"\",\r\n            normalImage:[\r\n                require('../../assets/ic_weixin_normal.png'),\r\n                require('../../assets/ic_contacts_normal.png'),\r\n                require('../../assets/ic_find_normal.png'),\r\n                require('../../assets/ic_me_normal.png'),\r\n                ],\r\n            selectImage:[\r\n                require('../../assets/ic_weixin_selected.png'),\r\n                require('../../assets/ic_contacts_selected.png'),\r\n                require('../../assets/ic_find_selected.png'),\r\n                require('../../assets/ic_me_selected.png'),\r\n                ],\r\n            active:\"message\",\r\n            msg:\"微信(2)\",\r\n            // rfImg:'../../../assets/.png',\r\n            //这个require有动态生成返回值\r\n            rsImg:require('../../assets/ic_add.png'),\r\n        }\r\n    },\r\n    methods:{\r\n        search(){\r\n            console.log(\"搜索\");\r\n        },\r\n        add3(){\r\n             console.log(\"添加\");\r\n        },\r\n        changeState(e){\r\n            for (var elem of this.currentIndex){\r\n                elem.isSelect=false;\r\n            }\r\n            this.currentIndex[e].isSelect=true;\r\n        }\r\n    },\r\n    components:{\r\n        tabIcon,\r\n        messageList,\r\n        titleBar,\r\n        // \"titleBar\":titleBar,\r\n        // titleBar:titleBar,\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n    /* 修改组件tabbar默认样式 */\r\n    .mint-tabbar>.mint-tab-item{\r\n        color:#999;\r\n        padding:20px 0;\r\n    }\r\n    /* 修改组件tabbar选中时的默认样式 */\r\n    .mint-tabbar>.mint-tab-item.is-selected{\r\n        color:#45c018;\r\n    }\r\n</style>"]}]}